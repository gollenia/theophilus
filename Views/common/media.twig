<div id="MediaApp" class="uk-modal-full" uk-modal>

 	<div class="uk-modal-dialog">
 	  
      <button class="uk-modal-close-full uk-close-large" type="button" uk-close></button>

    
    
		<div>
			<input type="file" id="uploadFile">
			<a class="uk-button" @click="uploadImage">Ulpoad!</a>
		</div>
		
		
			
			<div class="uk-child-width-1-2@m uk-child-width-1-3@l" uk-grid>
				<div v-for="image in imageList">
			        <a @click="returnImage(image.id)" class="uk-inline-clip uk-transition-toggle">
			            <img :src="'/lib/exe/fetch.php?media=' + image.id" alt="">
			            <div class="uk-transition-slide-bottom uk-position-bottom uk-overlay uk-overlay-default" tabindex="0">
    					<p class="uk-h4 uk-margin-remove" v-text="image.file"></p>
						</div>
			        </a>
				</div>
			    
			   
			</div>
	
        </div>     
  
</div>
	
		
